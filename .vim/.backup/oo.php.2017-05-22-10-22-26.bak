	/*
	 * 加载特殊开关，这里暂时仅有openProxy，并且config/config.switch.php是由其他程序自动维护
	 */
	static function addSwitchConfig($mid,$filefix,$fx=false){
		if(of::isBoyaaIp()){//博雅ip的用户后台控制是否走防攻击
			$cfg = oo::mobileConfig()->getDeploySite(oo::$config['sid'], 'attack');
			if($cfg){
				$dev_open = (array)$cfg['dev_open'];//内网打开
				$dev_close = (array)$cfg['dev_close'];//内网关闭
				$rel_open = (array)$cfg['rel_open'];//外网打开
				$rel_close = (array)$cfg['rel_close'];//外网关闭
				$mids = PRODUCTION_SERVER ? array_merge($rel_open, $rel_close) : array_merge($dev_open, $dev_close);
				$open_mid = PRODUCTION_SERVER ? $rel_open : $dev_open;
				
				if(in_array($mid, $mids) && oo::$config['sid'] == 108){//在数组里面就由这里的控制了，否则走正常逻辑
					if(in_array($mid, $open_mid)){//强制打开防攻击
						oo::$config['gps'] = 1;
						oo::$config['openProxy'] = 2;
						oo::$config['useWan'] = 0;
					}
					return true;
				}
			}
		}
		
	oo::addConfig('switch'); //防攻击开关
	return;

		if(oo::$config['sid']!=117 || !PRODUCTION_SERVER){
			oo::addConfig('switch'); //防攻击开关
		}else{
			if( of::isBoyaaIp() ){
				oo::addConfig('switch'); //防攻击开关
			}else{
				/*$wh=$mid%10;
				if($wh==1||$wh==2){
					oo::addConfig('switch'); 
				}*/
			}
		}
	}
	/**
	 * 根据文件前缀添加全局配置变量.
	 */
	static function addConfig( $filefix, $fx = false ){
		if( $fx ){//与原来的逻辑独立运行先，稳定后再合并
			$file = PATH_CFG . $filefix;
			if( !file_exists( $file ) ){
				$file = CFGROOT . $filefix;
			}
			if( file_exists( $file ) ){
				$ret = include_once($file);
				if( $ret === true ) return;//已包含过
				if( $ret && is_array( $ret ) && is_string( $fx ) ) $config = array( $fx => $ret );
				$config && self::setConfig( $config );
			}
			return;
		}
		$file = PATH_CFG . "config.{$filefix}.php";
		if( file_exists( $file ) ){
			include( $file);
			self::setConfig( $config );
		}
	}

	/**
	 * 设置语言文件
	 */
	static function setLang($langtype,$sid = 0){
		if(!isset(self::$oo['setLang'])){
			$sid = $sid ? $sid : oo::$config['sid'];
			$arr = array(93);
			$langfix = self::$config['language'][$langtype];
			self::$oo['setLang'] = isset($langfix) && !in_array(oo::$config['sid'], $arr) ? $langfix : 'zh'; //默认为中文
			isset($langfix) && self::$config['langtype'] = $langtype;
			$fix = in_array(oo::$config['sid'], $arr) ? '.'. oo::$config['sid'] : '';
			if(oo::$config['sid'] == 13 && $langtype == 1){//繁体平台的简体语言
				self::$oo['setLang'] = 'zh';
				$fix = '.13';
			}

			$fLang  = PATH_LNG . self::$oo['setLang'] . $fix .'.php';
			if(file_exists($fLang)){
				include_once($fLang);
			}
			oo::$config['sid'] == 144 && $langtype == 1 && oo::logs()->debug(date("Y-m-d His")." path:$fLang, exist:".file_exists($fLang), 'setLang.log');
		}
	}

