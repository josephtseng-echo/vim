	/**
	 * 模块加载，如oo::model('Member')->
	 * @return Model$name
	 */
	static function model( $name, $par = '' ){
		return self::obj($name, $par, 'activity/');
	}

	/**
	 * 模块加载，如 oo::obj('Minfo') 等效于 oo::minfo()
	 * @param $name
	 * @param string $par
	 * @param string $path
	 * @return Model$name
	 */
	static function obj($name, $par = '', $path = ''){
		$k = 'obj_'. $path .'_'. $name;
		if( !is_object(self::$oo[$k])){
			include_once PATH_MOD . $path . strtolower($name) . '.php';//文件名统一使用小写
			$cls = 'Model'.$name;
			self::$oo[$k] = new $cls($par);
		}
		return self::$oo[$k];
	}

	//与cmslib共用的类加载方法，如 oo::lib('ip') 对应 bylib/ip.class.php 类名为o_ip
	static function lib($k, $p = ''){
		$key = 'o_'. $k;
		if(!is_object(self::$oo[$key])){
			if(LOCAL === true || SERVER_TYPE === 'vm'){//本地
				$dir = '/data/wwwroot/cmslib/bylib/';
			}else{
				$dir = BY_LIB;
			}
			$f = $dir . $k .'.class.php';
			if(!is_file($f)) return false;
			include_once($f);
			self::$oo[$key] = new $key($p);
