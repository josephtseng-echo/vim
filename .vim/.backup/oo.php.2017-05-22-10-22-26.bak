	/**
	 * 活动模块加载，如oo::act($aid, $sid)->
	 * @return Act$aid
	 */
	static function act($aid, $_sid = ''){
		$sid = $_sid ? $_sid : oo::$config['sid'];
		if( in_array(oo::$config['sid'] ,array(117,144))){//人人合服 活动只上小站处理
			$sid = BY::toCmsSid($sid);
		}
		$path = WWWROOT .'act/'. $aid .'/';
		$cfgfile = $path .'config/'. $sid .'.php';
		if(!file_exists($cfgfile) && $_sid !== oo::$config['sid']){
			$sid = oo::$config['sid'];
			$cfgfile = $path .'config/'. $sid .'.php';
		}
		$k = 'act.'. $aid .'.'. $sid;
		if(!is_object(self::$oo[$k])){
			$f = $path .'model.php';
			if(!file_exists($f)) return false;
			include_once $f;
			$cls = 'Act'. $aid;
			self::$oo[$k] = new $cls($aid, $sid);//实例化类
			self::$oo[$k]->id = $aid;
			self::$oo[$k]->sid = $sid;
			self::$oo[$k]->childPath = $sid;
			self::$oo[$k]->cfg = file_exists($cfgfile) ? include($cfgfile) : array();
			if(method_exists(self::$oo[$k], '__init')) self::$oo[$k]->__init($aid, $sid);//自定义初始化函数
		}
		return self::$oo[$k];
	}

