(defconst *spell-check-support-enabled* nil)
(defconst *is-a-mac* (eq system-type 'darwin))
(setq custom-file (expand-file-name "custom.el" user-emacs-directory))



(require 'init-utils)
(my-add-subdirs-to-load-path my-emacs-vendor-lisps-path)

