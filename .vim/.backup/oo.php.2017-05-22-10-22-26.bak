			self::$oo['PHPMailer' . $pf]->Host = $host; //SMTP服务器地址
			self::$oo['PHPMailer' . $pf]->Port = $port; //SMTP服务器端口
			self::$oo['PHPMailer' . $pf]->Username = $username; //SMTP账号
			self::$oo['PHPMailer' . $pf]->Password = $password; //SMTP密码
			self::$oo['PHPMailer' . $pf]->SMTPSecure = $SMTPSecure; //安全验证:ssl/tls/''
			self::$oo['PHPMailer' . $pf]->AuthType = $authType; //SLOGIN | PLAIN | NTLM | CRAM-MD5 (default LOGIN)
			self::$oo['PHPMailer' . $pf]->Hostname = 'boyaa.com'; //指定发送服务器地址
			self::$oo['PHPMailer' . $pf]->SetFrom($from, (!empty(oo::$config['mailSender']) && is_string(oo::$config['mailSender'])) ? oo::$config['mailSender'] : 'Boyaa Interactive', true); //发送者.一般要与Username一致否则发不出
			self::$oo['PHPMailer' . $pf]->AddReplyTo($from, (!empty(oo::$config['mailSender']) && is_string(oo::$config['mailSender'])) ? oo::$config['mailSender'] : 'Boyaa Interactive'); //回复时的邮箱地址
		}
		return self::$oo['PHPMailer' . $pf];
	}

